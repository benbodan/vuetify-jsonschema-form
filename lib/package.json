{
  "name": "@koumoul/vjsf",
  "version": "3.0.0-alpha.3",
  "description": "Generate forms for the vuetify UI library (vuejs) based on annotated JSON schemas.",
  "scripts": {
    "test": "vitest",
    "build": "vue-tsc",
    "watch:build": "vue-tsc --watch"
  },
  "author": "Alban Mouton <alban.mouton@gmail.com>",
  "license": "MIT",
  "files": [
    "types",
    "src",
    "LICENSE"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "default": "./src/index.js",
        "types": "./types/index.d.ts"
      }
    },
    "./components/*": {
      "import": {
        "default": "./src/components/*",
        "types": "./types/components/*.d.ts"
      }
    },
    "./composables/*": {
      "import": {
        "default": "./src/composables/*",
        "types": "./types/composables/*.d.ts"
      }
    },
    "./compile": {
      "import": {
        "default": "./src/compile/index.js",
        "types": "./types/compile/index.d.ts"
      }
    },
    "./compat/v2": {
      "import": {
        "default": "./src/compat/v2.js",
        "types": "./types/compat/v2.d.ts"
      }
    }
  },
  "peerDependencies": {
    "vue": "^3.3.4",
    "vuetify": "^3.4.2"
  },
  "dependencies": {
    "@json-layout/core": "0.5.0",
    "@vueuse/core": "^10.5.0",
    "debug": "^4.3.4",
    "easymde": "^2.18.0",
    "ejs": "^3.1.9",
    "rfdc": "^1.3.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.8",
    "@types/ejs": "^3.1.2",
    "vitest": "^0.34.5",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.15"
  }
}
